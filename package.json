  "scripts": {
    "dev": "ARC_DB_PATH=./db pnpm dev:fresh",
    "dev:fresh": "run-p dev:backend dev:frontend",
    "dev:backend": "run-p dev:watch-source-refresh-lambda dev:backend:sandbox",
    "dev:watch-source-refresh-lambda": "node scripts/watchSourceRefreshLambda.cjs",
    "dev:backend:sandbox": "cd apps/backend && pnpm arc sandbox",
    "dev:update-env": "cd apps/backend && node ../../scripts/updateEnv.js",
    "dev:frontend": "run-p dev:frontend:app dev:frontend:styles",
    "dev:frontend:app": "cd apps/frontend && vite && pnpm @tailwindcss/cli -i ./src/index.css -o ./src/output.css --watch",
    "dev:frontend:styles": "pnpm tailwindcss -i ./apps/frontend/src/input.css -o ./apps/frontend/src/output.css --watch",
    "build": "pnpm build:frontend",
    "build:frontend": "cd apps/frontend && tsc -b && vite build",
    "preview:frontend": "cd apps/frontend && vite preview",
    "codegen": "graphql-codegen",
    "typecheck": "tsc --noEmit",
    "test": "pnpm test:frontend && pnpm test:libs",
    "test:frontend": "cd apps/frontend && vitest run",
    "test:libs": "pnpm vitest run libs",
    "test:e2e": "NODE_ENV=test E2E_NO_UI=true playwright test",
    "test:e2e:ui": "NODE_ENV=test playwright test --ui",
    "test:e2e:headed": "NODE_ENV=test playwright test --headed",
    "test:e2e:debug": "NODE_ENV=test playwright test --debug",
    "test:e2e:install": "NODE_ENV=test playwright install chromium",
    "discord:register-commands": "node dist/scripts/register-discord-commands.js",
    "lint": "pnpm eslint",
    "i18n:extract": "pnpm lingui extract --clean",
    "i18n:compile": "pnpm lingui compile --namespace es",
    "test:version-bump": "node scripts/test-version-bump.js"
  },
